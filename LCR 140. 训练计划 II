# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next
class Solution:
    def trainingPlan(self, head: Optional[ListNode], cnt: int) -> Optional[ListNode]:
        dummy = ListNode(-1)
        dummy.next = head
        p1 = dummy
        for _ in range(cnt):
            p1 = p1.next
        p2 = dummy
        while p1:
            p2 = p2.next
            p1 = p1.next
        return p2
